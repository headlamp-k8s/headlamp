{
  "name": "headlamp",
  "version": "0.1.0",
  "private": true,
  "productName": "Headlamp",
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.0.2",
    "@iconify/icons-mdi": "^1.1.10",
    "@iconify/react": "^1.1.3",
    "@kinvolk/eslint-config": "^0.5.0",
    "@material-ui/core": "^4.12.1",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@monaco-editor/react": "^4.2.1",
    "@octokit/core": "^3.5.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^12.0.0",
    "@types/javascript-time-ago": "^2.0.3",
    "@types/js-yaml": "^4.0.2",
    "@types/lodash": "^4.14.171",
    "@types/node": "^14.17.5",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.18",
    "@types/react-router-dom": "^5.1.8",
    "@types/semver": "^7.3.8",
    "@types/webpack-env": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "ansi-to-react": "^6.1.4",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^1.1.2",
    "github-markdown-css": "^4.0.0",
    "i18next": "^20.2.4",
    "i18next-browser-languagedetector": "^6.1.1",
    "if-env": "^1.0.4",
    "javascript-time-ago": "^2.3.8",
    "js-base64": "^3.6.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "monaco-editor": "^0.25.2",
    "notistack": "^1.0.3",
    "openapi-types": "^9.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hotkeys-hook": "^3.0.3",
    "react-i18next": "^11.11.1",
    "react-markdown": "^6.0.2",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "recharts": "^2.0.10",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "rimraf": "^3.0.2",
    "semver": "^7.3.5",
    "typescript": "^4.3.5",
    "xterm": "^4.10.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "if-env PUBLIC_URL react-scripts build || cross-env PUBLIC_URL=./ react-scripts build --max_old_space_size=768 && rimraf build/frontend/index.baseUrl.html",
    "test": "cross-env TEST_TZ=true react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint -c package.json --ext .js,.ts,.tsx src/ ../app/electron",
    "format": "prettier --config package.json --write src ../app/electron",
    "format-check": "prettier --config package.json --check src ../app/electron",
    "storybook": "start-storybook -p 6006 -s public",
    "build-typedoc": "npx typedoc",
    "build-storybook": "build-storybook -s public -o ../docs/development/storybook",
    "i18n": "npx --no-install i18next ./src/**/ts* ./src/**/**/*.ts* ./src/**/**/**/*.ts* -c ./src/i18n/i18next-parser.config.js",
    "tsc": "tsc"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint -c package.json --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --config package.json --write"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@kinvolk",
      "prettier",
      "plugin:jsx-a11y/recommended"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/build/",
      "!src/**/*.stories*.{js,jsx,ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 20,
        "functions": 18,
        "lines": 22,
        "statements": 22
      }
    },
    "coverageReporters": [
      "text"
    ]
  },
  "prettier": "@kinvolk/eslint-config/prettier-config",
  "devDependencies": {
    "@axe-core/react": "^4.2.2",
    "@storybook/addon-actions": "^6.3.4",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/addon-storyshots": "^6.3.4",
    "@storybook/node-logger": "^6.3.4",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.4",
    "eslint": "^7.30.0",
    "http-proxy-middleware": "^2.0.1",
    "husky": "^4.3.8",
    "i18next-parser": "^4.2.0",
    "lint-staged": "^10.5.4",
    "monaco-editor-webpack-plugin": "^4.0.0",
    "prettier": "^2.3.2",
    "typedoc": "^0.21.4",
    "typedoc-hugo-theme": "^0.2.1",
    "typedoc-plugin-markdown": "^3.10.3"
  }
}
