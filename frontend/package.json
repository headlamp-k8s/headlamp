    "@storybook/theming": "^8.2.9",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/user-event": "^14.5.2",
    "@types/nock": "^11.1.0",
    "@vitest/coverage-istanbul": "^2.0.5",
    "fetch-mock": "^9.11.0",
    "http-proxy-middleware": "^2.0.1",
    "husky": "^4.3.8",
    "i18next-parser": "^7.9.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^10.5.4",
    "msw": "2.3.5",
